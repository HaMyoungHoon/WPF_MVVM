<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/MaterialDesignColors;component/Themes/MaterialDesignColor.LightBlue.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="LightBlue1" Color="{StaticResource Primary100}" po:Freeze="True" />
    <SolidColorBrush x:Key="LightBlue1F" Color="{StaticResource Primary100Foreground}" po:Freeze="True" />
    <SolidColorBrush x:Key="LightBlue2" Color="{StaticResource Primary500}" po:Freeze="True" />
    <SolidColorBrush x:Key="LightBlue2F" Color="{StaticResource Primary500Foreground}" po:Freeze="True" />
    <SolidColorBrush x:Key="LightBlue3" Color="{StaticResource Primary700}" po:Freeze="True" />
    <SolidColorBrush x:Key="LightBlue3F" Color="{StaticResource Primary700Foreground}" po:Freeze="True" />
    
</ResourceDictionary>